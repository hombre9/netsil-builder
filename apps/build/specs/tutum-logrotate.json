{
 "id": "tutum-logrotate",
  "cpus": 0.2,
  "mem": 128,
  "cpus": 1.0,
  "mem": 16384,
  "instances": 1,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "acceptedResourceRoles": ["*"],
  "upgradeStrategy" : {
      "minimumHealthCapacity" : 0,
      "maximumOverCapacity" : 0
  },
  "env": {
    "DO_SCALE": "yes"
  },
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/lib/docker/containers",
        "hostPath": "/var/lib/docker/containers",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "tutum/logrotate:latest",
      "network": "BRIDGE",
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  }
}
