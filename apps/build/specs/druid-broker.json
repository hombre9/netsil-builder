{
 "id": "druid-broker",
 "container": {
     "type": "DOCKER",
     "volumes": [],
     "docker": {
         "network": "BRIDGE",
         "image": "netsilprivate/druid-broker:master-1.10.3-4390591.551",
         "forcePullImage": false,
         "portMappings": [{
             "containerPort": 31004,
             "hostPort": 31004,
             "protocol":"tcp"
         }]
     }
 },
  "cpus": 1.0,
  "mem": 16384,
  "instances": 1,
  "acceptedResourceRoles": ["*"],
  "constraints": [
      ["hostname", "UNIQUE"]
  ],
  "upgradeStrategy" : {
      "minimumHealthCapacity" : 0,
      "maximumOverCapacity" : 0
  },
  "uris": ["file:///etc/docker.tar.gz"],
  "env": {
    "DO_SCALE": "yes",
    "NODE_TYPE": "broker",
    "PRODUCT_FLAVOR": "Pro",
    "DEPLOY_ENV": "marathon",
    "druid_service": "druid-broker",
    "druid_port": "31004",
    "COLLECT_DRUID_METRICS": "no"
  }
}

